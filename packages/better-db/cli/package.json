{
  "name": "@better-db/cli",
  "version": "1.3.34",
  "description": "CLI for better-db schema generation and migration",
  "type": "module",
  "license": "MIT",
  "homepage": "https://github.com/olliethedev/better-auth/tree/main/packages/better-db#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/olliethedev/better-auth",
    "directory": "packages/better-db/cli"
  },
  "keywords": [
    "cli",
    "database",
    "schema",
    "migration",
    "typescript",
    "better-db"
  ],
  "publishConfig": {
    "access": "public",
    "executableFiles": [
      "./dist/index.mjs"
    ]
  },
  "scripts": {
    "build": "unbuild --clean",
    "stub": "unbuild --stub",
    "start": "node ./dist/index.mjs",
    "dev": "tsx ./src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --project tsconfig.json"
  },
  "main": "./dist/index.mjs",
  "exports": "./dist/index.mjs",
  "bin": {
    "better-db": "./dist/index.mjs",
    "@better-db/cli": "./dist/index.mjs"
  },
  "dependencies": {
    "@better-db/core": "workspace:*",
    "commander": "^12.1.0",
    "chalk": "^5.6.2",
    "jiti": "^2.4.2",
    "yocto-spinner": "^0.2.3",
    "prompts": "^2.4.2",
    "@mrleebo/prisma-ast": "^0.13.0",
    "prettier": "^3.6.2"
  },
  "peerDependencies": {
    "better-auth": ">=1.0.0"
  },
  "optionalDependencies": {
    "pg": "^8.14.0",
    "mysql2": "^3.12.0",
    "better-sqlite3": "^12.2.0"
  },
  "devDependencies": {
    "better-auth": "workspace:*",
    "kysely": "^0.28.5",
    "drizzle-orm": "^0.38.2",
    "@prisma/client": "^5.22.0",
    "@types/prompts": "^2.4.9",
    "@types/better-sqlite3": "^7.6.13",
    "@types/pg": "^8.11.10",
    "@types/node": "^20.0.0",
    "tsx": "^4.20.5",
    "typescript": "catalog:",
    "unbuild": "catalog:",
    "vitest": "catalog:"
  },
  "files": [
    "dist",
    "README.md"
  ]
}
