{
  "name": "@better-db/core",
  "version": "1.3.28",
  "description": "Core database utilities and schema definition for better-db",
  "type": "module",
  "license": "MIT",
  "homepage": "https://github.com/better-auth/better-auth/tree/main/packages/better-db#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/olliethedev/better-auth",
    "directory": "packages/better-db/core"
  },
  "keywords": [
    "database",
    "schema",
    "typescript",
    "better-db",
    "orm-agnostic"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "unbuild --clean",
    "stub": "unbuild --stub",
    "test": "vitest",
    "typecheck": "tsc --project tsconfig.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "dependencies": {
    "@better-auth/core": "workspace:*",
    "better-auth": "workspace:*",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "typescript": "catalog:",
    "unbuild": "catalog:",
    "vitest": "catalog:"
  },
  "files": [
    "dist",
    "README.md"
  ]
}
